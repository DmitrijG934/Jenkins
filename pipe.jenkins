#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent {
        label 'master'
    }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '1'))
        timestamps()
    }
    stages {
        stage("Preparation") {
            steps {
                sh 'ssh root@ubuntu \'hostname\''
            }
        }
        stage("Log Docker version") {
            steps {
                sh 'ssh root@ubuntu \'docker version\''
            }
        }
    }
}